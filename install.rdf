   - file, You can obtain one at http://mozilla.org/MPL/2.0/. --><RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>ssl-version-control@mozilla.org</em:id>
    <em:version>0.2</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>false</em:unpack>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.35.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>SSL Version Control</em:name>
    <em:description>Adds a simple preference to set the minimum SSL version that Firefox will accept.</em:description>
    <em:creator>Mozilla</em:creator>

    <em:optionsURL>chrome://ssl-version-control/content/options.xul</em:optionsURL>
    <em:optionsType>2</em:optionsType>

  </Description>
</RDF>
